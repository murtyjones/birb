{
  "name": "react-redux-typescript-boilerplate",
  "version": "1.0.1",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "homepage": "https://rokoroku.github.io/react-redux-typescript-boilerplate",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "boilerplate"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest",
    "start": "webpack-dev-server --env.BIRB_API_URL=http://localhost:8000  --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack --env.BIRB_API_URL=https://api.birb.io -p --progress --colors",
    "prettier": "prettier --write \"js/**/*.{ts,tsx,css}\"",
    "tslint": "tslint -c tslint.json js/**/*.ts --exclude 'js/**/*.d.ts'",
    "tslint-fix": "tslint -c tslint.json js/**/*.ts --exclude 'js/**/*.d.ts' --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-actions": "2.6.1",
    "@types/webpack": "^4.32.0",
    "@types/webpack-env": "1.14.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^4.1.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss": "^7.0.17",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "raf": "^3.4.1",
    "react-hot-loader": "^4.12.9",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deox": "^2.1.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0"
  }
}
